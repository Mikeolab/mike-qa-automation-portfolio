{
  "name": "tcall-senior-qa",
  "private": true,
  "version": "1.0.0",
  "description": "Senior QA Testing Suite for Tcall API",
  "scripts": {
    "test": "cypress run",
    "test:open": "cypress open",
    "test:dev": "cypress run --config-file cypress.config.senior.qa.js --env ENVIRONMENT=dev",
    "test:staging": "cypress run --config-file cypress.config.senior.qa.js --env ENVIRONMENT=staging",
    "test:prod": "cypress run --config-file cypress.config.senior.qa.js --env ENVIRONMENT=production",
    "test:smoke": "cypress run --spec 'cypress/e2e/tcall.smoke.cy.js'",
    "test:senior": "cypress run --spec 'cypress/e2e/tcall.senior.qa.cy.js'",
    "test:workflow": "cypress run --spec 'cypress/e2e/tcall.workflow.cy.js'",
    "test:onboarding": "cypress run --spec 'cypress/e2e/full_onboarding/*.cy.js'",
    "test:onboarding:workflow": "cypress run --spec 'cypress/e2e/full_onboarding/tcall.onboarding.workflow.cy.js'",
    "test:onboarding:security": "cypress run --spec 'cypress/e2e/full_onboarding/tcall.onboarding.security.cy.js'",
    "test:onboarding:performance": "cypress run --spec 'cypress/e2e/full_onboarding/tcall.onboarding.performance.cy.js'",
    "test:all": "cypress run --spec 'cypress/e2e/*.cy.js'",
    "test:comprehensive": "cypress run --spec 'cypress/e2e/tcall.comprehensive.all.endpoints.cy.js'",
    "test:comprehensive:api": "cypress run --config-file cypress.config.comprehensive.js --spec 'cypress/e2e/tcall.comprehensive.all.endpoints.cy.js'",
    "test:security": "cypress run --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'Security Testing Workflow'",
    "test:api": "cypress run --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'API Functionality Testing Workflow'",
    "test:performance": "cypress run --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'Performance Testing Workflow'",
    "test:integration": "cypress run --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'Integration Testing Workflow'",
    "test:debug": "cypress run --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'Debugging & Troubleshooting Workflow'",
    "test:parallel": "cypress run --parallel --record --key YOUR_CYPRESS_RECORD_KEY",
    "test:headless": "cypress run --headless",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    "clean": "rm -rf cypress/screenshots/* cypress/videos/* cypress/downloads/* cypress/results/*",
    "report": "npm run test && npx mochawesome-merge cypress/results/*.json > cypress/results/combined.json && npx mochawesome-report-generator cypress/results/combined.json --reportDir cypress/reports",
    "lint": "eslint cypress/**/*.js",
    "lint:fix": "eslint cypress/**/*.js --fix",
    "debug:workflow": "cypress open --spec 'cypress/e2e/tcall.workflow.cy.js'",
    "debug:security": "cypress open --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'Security Testing Workflow'",
    "debug:api": "cypress open --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'API Functionality Testing Workflow'",
    "debug:performance": "cypress open --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'Performance Testing Workflow'",
    "debug:integration": "cypress open --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'Integration Testing Workflow'",
    "debug:troubleshooting": "cypress open --spec 'cypress/e2e/tcall.workflow.cy.js' --grep 'Debugging & Troubleshooting Workflow'"
  },
  "devDependencies": {
    "cypress": "^13.17.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.1",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "cypress-xpath": "^2.0.1",
    "cypress-real-events": "^1.7.6",
    "cypress-wait-until": "^1.7.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^2.2.5",
    "cypress-cucumber-preprocessor": "^6.0.1",
    "cypress-axe": "^1.5.0",
    "cypress-visual-regression": "^4.0.0",
    "cypress-retry": "^2.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tcall-senior-qa.git"
  },
  "keywords": [
    "cypress",
    "testing",
    "api",
    "qa",
    "automation",
    "tcall",
    "voice",
    "ai",
    "workflow",
    "security",
    "performance"
  ],
  "author": "Senior QA Team",
  "license": "MIT",
  "config": {
    "cypress_record_key": "YOUR_CYPRESS_RECORD_KEY",
    "cypress_project_id": "YOUR_CYPRESS_PROJECT_ID"
  }
}
