                    data:
                      phone_number_id: 123
                      phone_number: '+1234567890'
                      previous_user: user@example.com
                      unassigned_by: admin@example.com
                      unassigned_at: '2025-07-25T10:30:00Z'
                  summary: Phone number successfully unassigned
                MissingIDError:
                  value:
