  /api/assign-phone-numbers/:
    post:
      operationId: api_assign_phone_numbers_create
      description: Assign a phone number to a specific user
      summary: Assign Phone Number
      tags:
      - Phone Number Management
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/AssignPhoneNumber'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/AssignPhoneNumber'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/AssignPhoneNumber'
        required: true
      security:
      - bearerAuth: []
      responses:
        '200':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/AssignmentResponse'
              examples:
                SuccessResponse:
                  value:
                    success: true
                    message: Phone number +1234567890 successfully assigned to user@example.com
                    data:
                      phone_number_id: 123
                      phone_number: '+1234567890'
                      assigned_to: user@example.com
                      assigned_by: admin@example.com
                      assigned_at: '2025-07-25T10:30:00Z'
                  summary: Phone number successfully assigned
                AlreadyAssignedError:
                  value:
                    success: false
                    message: Phone number is already assigned to another@example.com
                    current_user: another@example.com
                  summary: Phone number already assigned
                NotFoundError:
                  value:
                    success: false
                    message: Phone number not found or inactive
                  summary: Phone number or user not found
          description: ''
        '400':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/AssignmentResponse'
          description: ''
        '404':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/AssignmentResponse'
          description: ''
        '500':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/AssignmentResponse'
          description: ''
