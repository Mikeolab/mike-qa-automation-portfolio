{
  "name": "medcor-healthcare-platform-e2e",
  "version": "1.0.0",
  "description": "Medcor Healthcare Platform E2E Testing Suite",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:open": "cypress open",
    "test:staging": "cypress run --env ENVIRONMENT=staging",
    "test:production": "cypress run --env ENVIRONMENT=production",
    "test:super-admin": "cypress run --spec 'cypress/e2e/medcor-super-admin-flow.cy.js'",
    "test:hospital-admin": "cypress run --spec 'cypress/e2e/medcor-hospital-admin-flow.cy.js'",
    "test:doctor": "cypress run --spec 'cypress/e2e/medcor-doctor-flow.cy.js'",
    "test:patient": "cypress run --spec 'cypress/e2e/medcor-patient-flow.cy.js'",
    "test:all": "cypress run --spec 'cypress/e2e/*.cy.js'",
    "test:comprehensive": "cypress run --spec 'cypress/e2e/medcor-super-admin-flow.cy.js,cypress/e2e/medcor-hospital-admin-flow.cy.js,cypress/e2e/medcor-doctor-flow.cy.js,cypress/e2e/medcor-patient-flow.cy.js'",
    "test:complete-endpoints": "cypress run --spec 'cypress/e2e/medcor-comprehensive-endpoint-testing.cy.js'",
    "clean": "rm -rf cypress/screenshots/* cypress/videos/* cypress/downloads/* cypress/results/*",
    "report": "npm run test && npx mochawesome-merge cypress/results/*.json > cypress/results/combined.json && npx mochawesome-report-generator cypress/results/combined.json --reportDir cypress/reports",
    "lint": "eslint cypress/**/*.js",
    "lint:fix": "eslint cypress/**/*.js --fix"
  },
  "keywords": [
    "medcor",
    "healthcare",
    "e2e",
    "testing",
    "cypress",
    "api",
    "automation"
  ],
  "author": "Medcor QA Team",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.17.0",
    "cypress-mochawesome-reporter": "^3.6.0"
  },
  "dependencies": {
    "puppeteer": "^21.0.0"
  }
}






