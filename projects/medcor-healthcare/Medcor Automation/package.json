  "author": "Medcor QA Team",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.17.0",
    "cypress-mochawesome-reporter": "^3.6.0"
  },
  "dependencies": {
    "puppeteer": "^21.0.0"
  }
